{
  "name": "error-explainer",
  "displayName": "Error Explainer",
  "description": "A VS Code extension to explain error messages clearly using AI.",
  "version": "0.0.1",
  "publisher": "error-explainer",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Smit1963/shubham-extension.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "error",
    "explainer",
    "ai",
    "groq"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.explainError",
        "title": "Explain Error"
      },
      {
        "command": "extension.setGroqApiKey",
        "title": "Error Explainer: Set API Key"
      },
      {
        "command": "extension.setEnvApiKey",
        "title": "Error Explainer: Set API Key in .env"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "extension.explainError",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.4",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.26.0",
    "mocha": "^9.2.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "node-fetch": "^2.6.7"
  }
}
                                                                                                                